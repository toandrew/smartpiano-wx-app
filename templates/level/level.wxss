.m-level {
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding: 20rpx 25rpx;
  white-space: nowrap;
  font-size: 30rpx;
}

.u-level {
  display: inline-flex;
  padding: 10rpx 30rpx;
  color: #959595;
}

.u-level.active {
  background: #8a4aff;
  border-radius: 50rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  color: #fff;
}
