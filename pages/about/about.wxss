/* pages/about/about.wxss */

.g-container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.g-container .m-welcome {
  display: flex;
  flex-direction: column;
  text-align: center;
  color: #aaa;
  padding: 300rpx 0;
}

.g-container .m-welcome text:nth-of-type(2) {
  color: #00f;
  position: fixed;
  left: 0;
  right: 0;
  margin: 0 auto;
  bottom: 100rpx;
}
